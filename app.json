{
   "name": "rm bg",
   "description": "Background Remover Bot.",
   "logo": "https://telegra.ph/file/4d370b478d90d98709398.jpg",
   "keywords": ["telegram", "group"],
   "repository": "https://github.com/Geektyper/background/tree/main",
   "stack": "heroku-22",
   "addons": [
      {
         "plan": "heroku-postgresql",
         "options": {
            "version": "13"
         }
      }
   ],
   "env": {
      "RMBG": {
         "description": "Obtain this value from https://www.remove.bg/dashboard#api-key.",
         "required": true,
         "value": "your_remove_bg_api_key_here"
      },
      "TOKEN": {
         "description": "Bot token obtained from @BotFather on Telegram.",
         "required": true,
         "value": "your_bot_token_here"
      }
   },
   "buildpacks": [
      {
         "url": "https://github.com/HellBoy-OP/heroku-buildpack-ffmpeg-latest"
      },
      {
         "url": "https://github.com/heroku/heroku-buildpack-google-chrome"
      },
      {
         "url": "https://github.com/heroku/heroku-buildpack-chromedriver"
      },
      {
         "url": "https://github.com/heroku/heroku-buildpack-apt.git"
      },
      {
         "url": "heroku/python"
      }
   ]
}
